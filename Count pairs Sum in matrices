	    unordered_map<int,int>mp;
	    for(int i=0; i<n; i++){
	        for(int j=0; j<n; j++){
	            int a = x-mat1[i][j];
	            mp[a]++;
	        }
	    }
	    int count = 0;
	     for(int i=0; i<n; i++){
	        for(int j=0; j<n; j++){
	            if(mp[mat2[i][j]]){
	                count++;
	            }
	            
	        }
	
	    
	}
	return count;
